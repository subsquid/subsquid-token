{
  "name": "subsquid-token",
  "version": "0.1.0",
  "description": "Subsquid ERC20 Token",
  "main": "index.js",
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.2.0",
    "dotenv": "^10.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.9.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.10",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.3",
    "hardhat": "^2.5.0",
    "hardhat-gas-reporter": "^1.0.4",
    "solidity-coverage": "^0.7.16",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "local:node": "hardhat node",
    "compile": "hardhat compile",
    "clean": "rimraf cache artifacts .openzeppelin",
    "deploy:local": "npm run clean && hardhat run --network localhost scripts/deploy.ts",
    "deploy:ropsten": "npm run clean && hardhat run --network ropsten scripts/deploy.ts",
    "deploy:rinkeby": "npm run clean && hardhat run --network rinkeby scripts/deploy.ts",
    "deploy-upgrade:local": "hardhat run --network localhost scripts/upgradeV1.ts",
    "deploy-upgrade:ropsten": "hardhat run --network ropsten scripts/upgradeV1.ts",
    "deploy-upgrade:rinkeby": "hardhat run --network rinkeby scripts/upgradeV1.ts",
    "test": "hardhat test",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/subsquid/subsquid-token.git"
  },
  "keywords": [
    "erc20"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/subsquid/subsquid-token/issues"
  },
